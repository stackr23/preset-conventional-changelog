{
  "name": "@stackr23/preset-conventional-changelog",
  "version": "1.1.6",
  "author": "Stefan Friedl",
  "scripts": {
    "test": "echo 'no testscript defined'",
    "lint": "eslint --fix --ext \".js,.jsx,.ts,.tsx\" . --quiet --format pretty && prettier **/*.{css,scss,md,json} --write"
  },
  "homepage": "https://github.com/stackr23/preset-conventional-changelog#readme",
  "bugs": "https://github.com/stackr23/preset-conventional-changelog/issues",
  "description": "Stackr23 preset for conventional-changelog",
  "private": false,
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bundleDependencies": false,
  "dependencies": {
    "compare-func": "^2.0.0",
    "q": "^1.5.1"
  },
  "deprecated": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "index.js",
    "src/*",
    "templates/*"
  ],
  "keywords": [
    "conventional-changelog",
    "stackr23",
    "preset",
    "config"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackr23/preset-conventional-changelog.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "devDependencies": {
    "@stackr23/config-eslint": "^2.0.0",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0"
  }
}
